<div class="flyntComponent themeLight" is="flynt-navigation-footer-columns">
  <div class="container centerMaxWidthContainer">
    <div class="columns">
      <div class="columns-menu">
        {% include "Partials/_menu.twig" with { items: menu.items } %}
      </div>
      <div class="columns-info">
        <div class="logo">
          <img class="logo-image" src="{{ logo.src }}" alt="{{ logo.alt }}">
        </div>
        <div class="social">
          {{ socialContentHtml|e('wp_kses_post') }}
          {% if social %}
            <ul class="socialList">
              {% for socials in social %}
                <li class="socialList-item">
                  <a class="socialList-link" href="{{ socials.url }}" aria-label="{{ socials.platform.label }}">
                    {{- socials.icon -}}
                  </a>
                </li>
              {% endfor %}
            </ul>
          {% endif %}
        </div>
      </div>
    </div>
  </div>
</div>
