{
  "name": "flynt",
  "version": "1.2.1",
  "main": "index.js",
  "repository": "git@github.com:flyntwp/flynt.git",
  "author": "bleech <hello@bleech.de>",
  "license": "MIT",
  "scripts": {
    "checkVersion": "check-node-version --package -p",
    "clean": "rimraf ./dist",
    "start": "run-s --silent checkVersion clean watch",
    "watch": "node bs-server.js",
    "build": "run-s --silent checkVersion clean lint build:production build:rev",
    "build:production": "cross-env NODE_ENV=production webpack --progress --display=minimal",
    "build:rev": "gulp rev",
    "lint": "run-p --silent lint:*",
    "lint:scripts": "cross-env eslint '**/*.js'",
    "lint:styles": "cross-env stylelint '**/*.scss'",
    "lint:php": "cross-env ./vendor/bin/phpcs",
    "lint:scripts:fix": "cross-env eslint '**/*.js' --fix",
    "lint:styles:fix": "cross-env stylelint '**/*.scss' --fix",
    "lint:php:fix": "cross-env ./vendor/bin/phpcbf",
    "release": "standard-version -a"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/runtime": "^7.10.4",
    "ansi-colors": "^4.1.1",
    "autoprefixer": "^9.8.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "body-scroll-lock": "^2.7.1",
    "browser-sync": "^2.26.7",
    "check-node-version": "^4.0.3",
    "console-polyfill": "^0.3.0",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.6.5",
    "countup.js": "^2.0.5",
    "cross-env": "^6.0.3",
    "css-loader": "^3.6.0",
    "document-register-element": "^1.14.3",
    "draggabilly": "^2.3.0",
    "easings-css": "^1.0.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "fancy-log": "^1.3.3",
    "feather-icons": "^4.28.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "gulp": "^4.0.2",
    "gulp-notify": "^3.2.0",
    "gulp-rev": "^9.0.0",
    "gulp-rev-napkin": "^0.1.0",
    "gulp-rev-rewrite": "^2.0.0",
    "intersection-observer": "^0.7.0",
    "jquery": "^3.5.1",
    "js-cookie": "^2.2.1",
    "lazysizes": "^5.2.2",
    "lodash": "^4.17.19",
    "mini-css-extract-plugin": "^0.8.2",
    "node-sass": "^7.0.0",
    "node-sass-glob-importer": "^5.3.2",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "objectFitPolyfill": "^2.3.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "plugin-error": "^1.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.2",
    "prettier-stylelint": "^0.4.2",
    "rellax": "^1.12.1",
    "replace-in-file": "^4.3.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "standard-version": "^7.1.0",
    "stylelint": "^11.1.1",
    "stylelint-config-sass-guidelines": "^6.2.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-scss": "^3.18.0",
    "swiper": "^5.4.5",
    "terser-webpack-plugin": "^2.3.7",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2"
  },
  "eslintConfig": {
    "extends": "eslint-config-standard",
    "rules": {
      "import/no-webpack-loader-syntax": 0
    }
  },
  "eslintIgnore": [
    "dist/**",
    "vendor/**"
  ],
  "stylelint": {
    "extends": "stylelint-config-sass-guidelines",
    "plugins": [
      "stylelint-scss"
    ],
    "rules": {
      "at-rule-no-unknown": null,
      "scss/at-rule-no-unknown": true,
      "selector-class-pattern": null,
      "max-nesting-depth": null,
      "selector-max-compound-selectors": 4
    },
    "ignoreFiles": [
      "dist/**",
      "vendor/**"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "standard-version": {
    "scripts": {
      "postbump": "gulp replaceVersion && composer config --unset version && composer update nothing && git diff && git add . && composer validate --strict"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.14.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie 10"
  ]
}
